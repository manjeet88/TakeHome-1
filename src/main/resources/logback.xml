<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern># %date{yyyy.MM.dd HH:mm:ss.SSSZ} [%-5level] [%thread]-[%c{0}.%M:%L] : %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="io.github.tinius" level="TRACE"/>

  <root level="INFO">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>